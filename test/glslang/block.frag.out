    mediump vec3 w;
    mediump mat3 m3a[5];
};

uniform S s;
uniform fooBlock {
    mediump vec4 bu;
    mediump vec2 bw;
    mediump uvec4 bv;
    S bs;
    mediump mat2 bm2;
    mediump mat4 m4a[8];
} ;
uniform barBlock {
    mediump vec4 nbu;
    mediump mat3 nbm;
    mediump uvec4 nbv;
    mediump int ni;
} inst[4];
out mediump vec4 color;

void main()
{
    mediump ivec4 temp21 = texture(s.sampler, bw);
    mediump vec4 temp22 = vec4(temp21);
    mediump vec4 temp23 = temp22 + inst[3].nbu;
    mediump vec4 color_shadow = temp23 + bu;
    color = color_shadow;
    
}

