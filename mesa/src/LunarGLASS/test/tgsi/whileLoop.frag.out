loop-header:                                      ; preds = %ifmerge, %entry
  %color.0 = phi <4 x float> [ %BaseColor, %entry ], [ %color1, %ifmerge ]
  %0 = extractelement <4 x float> %color.0, i32 0
  %1 = load float addrspace(1)* @d
